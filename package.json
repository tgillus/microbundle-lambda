{
  "name": "microbundle-lambda",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run clean && npm test && tsc",
    "clean": "del-cli lib dist",
    "bundle": "npm run clean && microbundle build -i src/lambda.ts -o dist/lambda.js --target node --format esm --generateTypes false --sourcemap --external oracledb",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepare": "husky install",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "author": "Tramaine L. Gillus",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "@vitest/coverage-c8": "0.28.3",
    "del-cli": "5.0.0",
    "eslint": "8.33.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "microbundle": "0.15.1",
    "prettier": "2.8.3",
    "tsx": "3.12.2",
    "typescript": "4.9.4",
    "vitest": "0.28.3"
  },
  "dependencies": {
    "dayjs": "1.11.7",
    "oracledb": "5.5.0"
  }
}
