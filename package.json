{
  "name": "microbundle-lambda",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run clean && npm test && tsc",
    "clean": "del-cli lib dist",
    "bundle": "npm run clean && microbundle build -i src/lambda.ts -o dist/lambda.js --target node --format esm --generateTypes false --sourcemap --external oracledb",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepare": "husky install",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "author": "Tramaine L. Gillus",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@types/node": "20.4.1",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "@vitest/coverage-c8": "0.33.0",
    "del-cli": "5.0.0",
    "eslint": "8.44.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "microbundle": "0.15.1",
    "prettier": "3.0.0",
    "tsx": "3.12.7",
    "typescript": "5.1.6",
    "vitest": "0.33.0"
  },
  "dependencies": {
    "dayjs": "1.11.9",
    "oracledb": "6.0.2"
  }
}
